{
  "name": "Freelance-Application",
  "version": "1.0.0",
  "description": "An application for developers to view and post jobs",
  "main": "server.js",
  "scripts": {
    "test2": "jasmine-node --autotest --config NODE_ENV test spec/routes.spec/*",
    "test": "jasmine-node --autotest --config NODE_ENV test spec/models.spec/",
    "start": "nodemon server.js",
    "prestart": "npm install",
    "postinstall": "bower install",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor protractor.conf.js"

  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-kochu/freelance.git"
  },
  "keywords": [
    "Freelance"
  ],
  "author": "Kingsley Ochu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-kochu/freelance/issues"
  },
  "homepage": "https://freelance-app.herokuapp.com",
  "dependencies": {
    "body-parser": "^1.12.3",
    "express": "^4.12.3",
    "bower": "^1.3.1",
    "express-session": "^1.11.1",
    "jsonwebtoken": "^5.0.0",
    "method-override": "^2.3.2",
    "mongoose": "^5.7.8",
    "mongoose-url-slugs": "^0.1.3",
    "morgan": "^1.5.2",
    "passport": "^0.2.1",
    "passport-google-oauth": "~0.1.5",
    "passport-linkedin": "^0.1.3",
    "passport-linkedin-oauth2": "^1.2.1",
    "passport-oauth": "^1.0.0"
  },
  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "supertest": "^0.15.0",
    "angular-mocks": "^1.3.15",
    "http-server": "^0.8.0",
    "jasmine-core": "^2.3.3",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.10",
    "karma-jasmine": "^0.3.5",
    "protractor": "^2.0.0"
  },
  "engines": {
    "node": "0.12.2",
    "npm": "2.7.4"
  }
}
